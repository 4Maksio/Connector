<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="using:Connector.ViewModels"
			 xmlns:m="using:Connector.Models"
			 x:Class="Connector.Views.CanvaView"
			 x:DataType="vm:CanvaViewModel">
	<StackPanel>
		<ItemsControl ItemsSource="{Binding BasicItems}">
			<ItemsControl.ItemTemplate>
				<DataTemplate DataType="m:BasicItem">
					<Border Margin="10" Height="50" Width="100" Background="BlueViolet">
						<TextBlock Text="{Binding Name}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
					</Border>
				</DataTemplate>
			</ItemsControl.ItemTemplate>
		</ItemsControl>
	</StackPanel>
</UserControl>
