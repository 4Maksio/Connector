<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="using:Connector.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="Connector.Views.DashboardView"
             x:DataType="vm:DashboardViewModel">
	<Grid RowDefinitions="auto *" ColumnDefinitions="auto *">
		<ContentControl Grid.ColumnSpan="2" Content="{Binding TopBar}"/>
		<ContentControl Grid.Row="1" Content="{Binding LeftPanel}"/>
		<ContentControl Grid.Column="1" Grid.Row="1" Content="{Binding Canva}"/>
	</Grid>
</UserControl>
