<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="using:Connector.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="Connector.Views.DashboardView"
             x:DataType="vm:DashboardViewModel">
	<Grid RowDefinitions="auto *" ColumnDefinitions="auto *" ShowGridLines="True">
		<StackPanel Classes="top-bar" Grid.ColumnSpan="2" Orientation="Horizontal" Spacing="5">
			<Button Classes="navigation">
				<Image Source="{SvgImage /Assets/Images/Sidebar-Toggle.svg}"/>
			</Button>
			<Image Height="26" Source="{SvgImage /Assets/Images/Logo-Full.svg}"/>
		</StackPanel>
		<StackPanel Grid.Row="1" Orientation="Vertical" Spacing="5">
			<Button>
				<Image Height="12" Source="{SvgImage /Assets/Images/Add-Object.svg}"/>
			</Button>
		</StackPanel>
		<StackPanel Grid.Column="1" Grid.Row="1" Orientation="Horizontal" Spacing="5">
		</StackPanel>
	</Grid>
</UserControl>
